{
  "name": "govuk-eleventy",
  "description": "Build websites using the GOV.UK design system and the Eleventy static site generator",
  "private": true,
  "keywords": [
    "govuk",
    "government",
    "11ty"
  ],
  "author": "Paul Robert Lloyd",
  "license": "MIT",
  "homepage": "https://govuk-eleventy.herokuapp.com",
  "engines": {
    "node": "^16.0.0"
  },
  "main": ".eleventy.js",
  "scripts": {
    "prebuild": "rimraf public",
    "build": "eleventy --quiet",
    "predev": "npm run build",
    "dev": "eleventy --serve --quiet",
    "prestart": "npm run build",
    "start": "http-server"
  },
  "workspaces": [
    "package"
  ],
  "dependencies": {
    "@11ty/eleventy": "^1.0.0-beta",
    "govuk-eleventy-plugin": "^0.1.0",
    "http-server": "^14.0.0",
    "rimraf": "^3.0.2"
  }
}
