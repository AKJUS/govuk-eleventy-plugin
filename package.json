{
  "name": "govuk-11ty",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "^16.0.0"
  },
  "description": "Build websites using the GOV.UK design system and the Eleventy static site generator",
  "keywords": [
    "govuk",
    "11ty"
  ],
  "author": "Paul Robert Lloyd",
  "license": "MIT",
  "homepage": "https://govuk-11ty.herokuapp.com",
  "repository": "github:paulrobertlloyd/govuk-11ty",
  "bugs": "https://github.com/paulrobertlloyd/govuk-11ty/issues",
  "scripts": {
    "prebuild": "rimraf public",
    "build": "eleventy --quiet",
    "predev": "npm run build",
    "dev": "eleventy --serve",
    "prestart": "npm run build",
    "start": "http-server",
    "lint": "standard && stylelint 'app/**/*.scss'"
  },
  "dependencies": {
    "@11ty/eleventy": "^1.0.0-beta.8",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.3",
    "govuk-frontend": "^3.4.0",
    "http-server": "^14.0.0",
    "luxon": "^2.1.1",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-block-embed": "^0.0.3",
    "markdown-it-deflist": "^2.0.3",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-image-figures": "^2.0.0",
    "markdown-it-ins": "^3.0.0",
    "markdown-it-mark": "^3.0.0",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-table-of-contents": "^0.6.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.60.0",
    "sass": "^1.43.4"
  },
  "devDependencies": {
    "standard": "^16.0.4",
    "stylelint": "^13.13.1",
    "stylelint-config-gds": "^0.1.0"
  },
  "standard": {
    "ignore": [
      "!.eleventy.js"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-gds/scss"
  }
}
